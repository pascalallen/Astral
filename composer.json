{
    "name": "pascalallen/astral",
    "type": "project",
    "description": "Astral",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "doctrine/cache": "^1.11",
        "doctrine/orm": "*",
        "league/tactician": "^1.1",
        "monolog/monolog": "^2.3",
        "psr/container": "^2.0",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.30.1",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Astral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astral\\Test\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0",
            "ext-sockets": "8.0.0"
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
